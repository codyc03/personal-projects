{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Node.js Server",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/server.js",
        "runtimeArgs": ["--inspect"],
        "env": {
          "NODE_ENV": "development"
        },
        "console": "integratedTerminal",
        "outputCapture": "std"
      }
    ]
  }
  